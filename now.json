{
  "name": "XITY Starter",
  "version": 2,
  "alias": [
    "www.xity.now.sh",
    "xity.now.sh"
  ],
  "scope": "equinusocio",
  "github": {
    "silent": true
  },
  "build": {
    "env": {
      "ELEVENTY_ENV": "production"
    }
  },
  "functions": {
    "includeFiles": "dist/"
  },
  "routes": [{
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/404.html"
    }
  ]
}
