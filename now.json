{
  "name": "XITY Starter",
  "version": 2,
  "alias": [
    "www.xity.now.sh",
    "xity.now.sh"
  ],
  "scope": "equinusocio",
  "github": {
    "silent": true
  },
  "builds": [{
    "src": "package.json",
    "use": "@now/static-build",
    "config": {
      "distDir": "dist"
    }
  }],
  "build": {
    "env": {
      "ELEVENTY_ENV": "production"
    }
  },
  "routes": [{
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/404.html"
    }
  ]
}
